{
    "name": "ptt-articles",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev:server": "cross-env NODE_ENV=development nodemon server --exec babel-node --watch server",
        "build:server": "cross-env NODE_ENV=production babel server -d dist/server",
        "dev:client": "cross-env NODE_ENV=development parcel src/index.html -d build --no-cache",
        "build:client": "cross-env NODE_ENV=production parcel build src/index.html -d dist/client --no-cache --public-url='./'",
        "start": "cross-env NODE_ENV=production node dist/server"
    },
    "dependencies": {
        "antd": "^3.1.3",
        "axios": "^0.17.1",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-plugin-import": "^1.6.3",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react-app": "^3.1.1",
        "babel-preset-stage-0": "^6.24.1",
        "body-parser": "^1.18.2",
        "cors": "^2.8.4",
        "cross-env": "^5.1.3",
        "express": "^4.16.2",
        "less": "^2.7.3",
        "multer": "^1.3.0",
        "parcel-bundler": "^1.4.1",
        "ptt-scr": "^1.1.13",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "serve-static": "^1.13.1"
    },
    "devDependencies": {
        "eslint": "^4.16.0",
        "nodemon": "^1.14.11"
    }
}
